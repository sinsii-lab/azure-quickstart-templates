	proc sql;
	create table rule_ids as
	select distinct authorization_rule_id from apdm.model_master 
	where 
	authorization_rule_id is not null
	union
	select distinct authorization_rule_id from apdm.project_master
	where 
	authorization_rule_id is not null
	;
	quit;
	
	%let count=&sqlobs.;
	
	%if &count. gt 0 %then %do;
		%do i=1 %to &count.;
			data _null_;
			pointobs=&i.;
			set rule_ids point=pointobs;
			call symputx('authorization_rule_id',authorization_rule_id);   /* i18nOK:LINE */
			stop;
			run;
		
			proc http url="&BASE_URI./authorization/rules/&authorization_rule_id."/* i18nOK:Line */
			 method='DELETE'/* i18nOK:Line */
			 oauth_bearer=sas_services
			 ct="application/json";/* i18nOK:Line */
