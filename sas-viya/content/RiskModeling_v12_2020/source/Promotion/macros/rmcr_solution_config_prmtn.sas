		select 
			case when upcase(load_status)='Y' then (select status_sk from apdm.status_master where upcase(status_cd)='SUCCESS')    /* i18nOK:LINE */
				else (select status_sk from apdm.status_master where upcase(status_cd)='FAIL') END    /* i18nOK:LINE */
			FROM 
			(select 
			/*scr.scoring_template_sk,
			ld_dt.score_dttm,*/
			ld_dt.load_status 
			from
			work.csb_score_load_dates ld_dt
			inner join 
			apdm.scoring_model scr
			on scr.scoring_model_id=ld_dt.scoring_model_id
			where scr_cntrl.scoring_template_sk=scr.scoring_template_sk
			and scr_cntrl.scoring_as_of_dttm=ld_dt.score_dttm
			));
					
					
		update &m_lib_apdm..actual_result_control_detail act_cntrl set load_csmart_status_sk=(
