			inner join  
			work.csb_score_load_dates scr_ld_dt
			on act_ld_dt.score_load_sk=scr_ld_dt.score_load_sk
			inner join 
			apdm.scoring_model scr
			on scr.scoring_model_id=scr_ld_dt.scoring_model_id
			inner join
			apdm.scoring_control_detail scr_cntrl_dtl
			on scr.scoring_template_sk=scr_cntrl_dtl.scoring_template_sk
			and scr_ld_dt.score_dttm=scr_cntrl_dtl.scoring_as_of_dttm
			where act_cntrl.scoring_control_detail_sk=scr_cntrl_dtl.scoring_control_detail_sk
