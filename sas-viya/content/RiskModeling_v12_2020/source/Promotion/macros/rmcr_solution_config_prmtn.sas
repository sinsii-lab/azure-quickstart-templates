			%put %sysfunc(sasmsg(work.rmcr_message_dtl_promotion, RMCR_PROMOTION_MSG.SOL_CONFIG_SM5.1, noquote,&authorization_rule_id.,&SYS_PROCHTTP_STATUS_CODE.,&SYS_PROCHTTP_STATUS_PHRASE.));
			%goto exit;
			%end;
			
		%end;
	%end;
	
	
